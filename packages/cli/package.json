{
  "name": "@conversationai/moderator-cli",
  "description": "Commands for OSMod",
  "version": "1.1.0",
  "scripts": {
    "lint": "find commands -name '*.ts' | xargs ../../node_modules/.bin/tslint -c ../../tslint.json",
    "lint:fix": "find commands -name '*.ts' | xargs ../../node_modules/.bin/tslint -c ../../tslint.json --fix",
    "build": "npm run compile",
    "compile": "../../node_modules/.bin/tsc --sourceMap --rootdir commands --outDir dist --declaration --skipLibCheck",
    "watch": "../../node_modules/.bin/tsc --sourceMap --rootdir commands --outDir dist --declaration --skipLibCheck --watch"
  },
  "bin": {
    "osmod": "./bin/osmod.js"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": "6.11.0"
  },
  "dependencies": {
    "@conversationai/moderator-backend-core": "1.1.0",
    "@conversationai/moderator-config": "1.1.0",
    "@types/js-yaml": "3.11.1",
    "@types/lodash": "4.14.120",
    "@types/mysql": "2.15.5",
    "@types/request": "2.47.0",
    "@types/sequelize": "3.4.43",
    "@types/yargs": "11.0.0",
    "axios": "0.18.0",
    "find-up": "2.1.0",
    "js-yaml": "3.12.0",
    "lodash": "4.17.11",
    "mysql": "2.15.0",
    "request": "2.87.0",
    "sequelize": "3.30.4",
    "sequelize-cli": "2.7.0",
    "yargs": "11.1.0"
  },
  "devDependencies": {
    "@types/express-serve-static-core": "^4.17.28",
    "@types/qs": "^6.9.7",
    "@types/sequelize": "4.28.1",
    "@types/validator": "10.11.3"
  }
}
